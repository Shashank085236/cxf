<?xml version="1.0" encoding="UTF-8"?>
<cm:config 
    xmlns:cm="http://celtix.objectweb.org/config-metadata" 
    xmlns:ct="http://celtix.objectweb.org/config-types" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://celtix.objectweb.org/configuration/metadata metadata.xsd">

   <!-- multiple items with same name are illegal  -->
   
    <cm:configItem>
		<cm:name>stringItem</cm:name>
		<cm:type>ct:string</cm:type>
	</cm:configItem>
	
	<cm:configItem>
		<cm:name>stringItem</cm:name>
		<cm:type>ct:string</cm:type>
	</cm:configItem>
	
</cm:config>