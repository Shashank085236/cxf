<?xml version="1.0"?>
<project name="hello world demo" default="build" basedir=".">

    <import file="../common_build.xml"/>

    <property name="wsdl.dir" location="${basedir}/wsdl"/>
    <property name="wsdl.file" value="hello_world_RPCLit.wsdl"/>

    <target name="client" description="run demo client">
        <celtixrun classname="demo.hwRPCLit.client.Client" wsdl="${basedir}/wsdl/hello_world_RPCLit.wsdl"/>
    </target>
    

    <target name="server" description="run demo server">
        <celtixrun classname="demo.hwRPCLit.server.Server" wsdl="${basedir}/wsdl/hello_world_RPCLit.wsdl"/>
    </target>
</project>
