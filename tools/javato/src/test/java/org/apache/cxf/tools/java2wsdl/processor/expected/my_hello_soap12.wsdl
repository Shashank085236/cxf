        <soap12:body use="literal"/>
      </wsdl:output>
      <wsdl:fault name="faultDetail">
        <soap12:fault name="faultDetail" use="literal"/>
      </wsdl:fault>
    </wsdl:operation>
  </wsdl:binding>
  <wsdl:service name="MyService">
